<mvc:View controllerName="cp.CopyPaste.controller.CopyPaste" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m" xmlns:c="cp.CopyPaste.control">
	<App>
		<Page title="Example of Copy Paste">
		<content>
			<c:CopyPasteTable id ='idTable' items="{/itemsPath}">
				<c:items> <!-- sap.m.ListItemBase -->
					<ColumnListItem id='idColListTemp' xmlns="sap.m"
						detailTap="" press="" detailPress="">
						<cells>
							
							<Input value="{fname}" >

							</Input>
							<Input value="{lname}" >

							</Input>
							<Input value="{company_name}" >

							</Input>
							<Input value="{email}" >

							</Input>
							<Input value="{country}" >

							</Input>
							<Input value="{phone}" >

							</Input>
						</cells> <!-- sap.ui.core.Control -->
					</ColumnListItem>

				</c:items>
				<c:columns> <!-- sap.m.Column -->
					
					<Column >
						<header>
							<Text text="First Name" >
								
								
							</Text>
						</header> <!-- sap.ui.core.Control -->
					</Column>
					<Column >
						<header>
							<Text text="Last Name" >
								
								
							</Text>
						</header> <!-- sap.ui.core.Control -->
					</Column>
					<Column >
						<header>
							<Text text="Company Name " >
							</Text>
						</header> <!-- sap.ui.core.Control -->
					</Column>
					<Column >
						<header>
							<Text text="Email">
								
								
							</Text>
						</header> <!-- sap.ui.core.Control -->
					</Column>
					<Column >
						<header>
							<Text  text="Country" >
								
								
							</Text>
						</header> <!-- sap.ui.core.Control -->
					</Column>
					<Column >
						<header>
							<Text text="Phone" >
								
								
							</Text>
						</header> <!-- sap.ui.core.Control -->
					</Column>
				</c:columns>
			</c:CopyPasteTable>
		</content>
	</Page>
	</App>
</mvc:View>