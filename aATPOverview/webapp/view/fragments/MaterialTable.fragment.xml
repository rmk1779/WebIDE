<core:FragmentDefinition xmlns="sap.ui.table" xmlns:m="sap.m" xmlns:core="sap.ui.core">
	<Table id="idMaterialTable" selectionMode="MultiToggle" enableSelectAll="false">
		<extension>
			<m:OverflowToolbar>
				<!--<m:Title id="title" text="{i18n>material}"/>-->
				<m:ToolbarSpacer/>
			</m:OverflowToolbar>
		</extension>
		<columns>
			<Column id="idColumnItem">
				<m:Label id="idColumnItemLabel" text="{i18n>material}"/>
				<template>
					<m:HBox>
						<m:Text id="idDataItem" text="{ATPOverviewModel>Item}" wrapping="false" class="sapUiSmallMarginTop"/>
					</m:HBox>
				</template>
			</Column>
			<Column id="idColumnItemDesc">
				<m:Label id="idColumnItemDescLabel" text="{i18n>description}"/>
				<template>
					<m:HBox>
						<m:Text id="idDataItemDesc" text="{ATPOverviewModel>Maktx}" wrapping="false" class="sapUiSmallMarginTop"/>
					</m:HBox>
				</template>
			</Column>
		</columns>
	</Table>
</core:FragmentDefinition>