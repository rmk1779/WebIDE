<core:FragmentDefinition xmlns:mvc="sap.ui.core.mvc" xmlns="sap.ui.comp.valuehelpdialog" xmlns:core="sap.ui.core" xmlns:layout="sap.ui.layout" xmlns:m="sap.m" xmlns:f="sap.f"
	xmlns:fb="sap.ui.comp.filterbar" xmlns:vm="sap.ui.comp.variants" xmlns:smartFilterBar="sap.ui.comp.smartfilterbar">
	<ValueHelpDialog title="Product" ok=".onValueHelpOkPress" cancel=".onValueHelpCancelPress" afterClose=".onValueHelpAfterClose"
		supportRanges="true" key="ProductId" descriptionKey="ProductName" >
		<filterBar>
			<fb:FilterBar advancedMode="true" search="onFilterBarSearch" showGoButton="true">
				<fb:filterGroupItems>
					<fb:FilterGroupItem groupName="__$INTERNAL$" name="ProductId" label="Product ID" visibleInFilterBar="true">
						<fb:control>
							<m:Input name="ProductId"/>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem groupName="__$INTERNAL$" name="ProductName" label="Product Name" visibleInFilterBar="true">
						<fb:control>
							<m:Input name="ProductName"/>
						</fb:control>
					</fb:FilterGroupItem>
					<fb:FilterGroupItem groupName="__$INTERNAL$" name="Category" label="Category" visibleInFilterBar="true">
						<fb:control>
							<m:Input name="Category"/>
						</fb:control>
					</fb:FilterGroupItem>
				</fb:filterGroupItems>
			</fb:FilterBar>
		</filterBar>
	</ValueHelpDialog>
</core:FragmentDefinition>